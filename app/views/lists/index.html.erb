<% @lists.each do |list| %>
  <h2><%= list.name%></h2>
  <p><%= list.description %></p>
  <%= link_to "see details", list_path(list)%>
<% end %>

<div data-controller="search-books">
  <%= form_with url: lists_path, method: :get do |f| %>
    <%= f.text_field :query,
                     class: "form-control",
                     placeholder: "Type keywords",
                     value: params[:query],
                     data: {search_books_target: "input",
                      action: "keyup->search-books#update"
                     }%>
  <% end %>
  <div data-search-books-target="searchResult">
  </div>
</div>
